{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "Translation key",
          "name": "translationKey",
          "type": "string"
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "string"
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Comments",
          "name": "comment",
          "type": "fields",
          "fields": [
            {
              "title": "Enable",
              "name": "enable",
              "type": "boolean",
              "default": "false"
            }
          ]
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "hiddenFromSearch",
          "name": "hiddenFromSearch",
          "type": "boolean"
        },
        {
          "title": "type",
          "name": "type",
          "type": "string"
        }
      ]
    },
    {
      "name": "posts",
      "pageBundle": true,
      "fields": [
        {
          "title": "Translation key",
          "name": "translationKey",
          "type": "string"
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "string"
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Subtitle",
          "name": "subtitle",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Last modified date",
          "name": "lastmod",
          "type": "datetime",
          "default": "{{now}}",
          "isModifiedDate": true
        },
        {
          "title": "Frontmatter Content preview",
          "name": "preview",
          "type": "image",
          "default": "featured-image.jpg"
        },
        {
          "title": "Comments",
          "name": "comment",
          "type": "fields",
          "fields": [
            {
              "title": "Enable",
              "name": "enable",
              "type": "boolean"
            }
          ]
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Table of contents",
          "name": "toc",
          "type": "boolean",
          "default": "false"
        },
        {
          "title": "Enable comments",
          "name": "comment",
          "type": "boolean",
          "default": "true"
        },
        {
          "title": "type",
          "name": "type",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.preview.host": "http://localhost:1314",
  "frontMatter.content.i18n": [
    {
      "title": "English",
      "locale": "en",
      "path": "en"
    },
    {
      "title": "German",
      "locale": "de",
      "path": "de"
    }
  ],
  "frontMatter.content.pageFolders": [
    {
      "title": "Pages",
      "path": "[[workspace]]/content/en",
      "contentTypes": [
        "default"
      ],
      "filePrefix": "",
      "defaultLocale": "en",
      "locales": [
        {
          "title": "English",
          "locale": "en",
          "path": "."
        },
        {
          "title": "German",
          "locale": "de",
          "path": "../de"
        }
      ]
    },
    {
      "title": "Blog posts",
      "path": "[[workspace]]/content/en/posts",
      "contentTypes": [
        "posts"
      ],
      "filePrefix": "",
      "defaultLocale": "en",
      "locales": [
        {
          "title": "English",
          "locale": "en",
          "path": "."
        },
        {
          "title": "German",
          "locale": "de",
          "path": "../../de/posts"
        }
      ]
    }
  ],
  "frontMatter.git.enabled": true,
  "frontMatter.website.host": "https://julian.pawlowski.me",
  "frontMatter.dashboard.openOnStart": true,
  "frontMatter.framework.startCommand": "hugo server --disableFastRender -D",
  "frontMatter.content.snippets": {
    "About the author": {
      "description": "About Julian Pawlowski",
      "body": "About the author text ....",
      "fields": []
    },
    "Über den Autor": {
      "description": "about",
      "body": "Über den Autor",
      "fields": []
    }
  }
}